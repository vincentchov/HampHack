{% extends "base.html.j2" %}
{% block title %}Index{% endblock %}
{% block head %}
  {{ super() }}
  <style type="text/css">
    .important { color: #336699; }

    #map {
        height: 100%;
        width: 100%;
        position: absolute;
        top: 0px;
        left: 0px;
       }

  </style>
{% endblock %}
{% block content %}

<div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
      </script>
<script async defer
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyArgGYqckJLb7caGS97EybJUbB6m6eDRdg&callback=initMap">
</script>
{% endblock %}
